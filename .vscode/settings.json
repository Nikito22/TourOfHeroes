// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": {
      "when": "$(basename)"
    },
    "**/*.spec.ts": {
      "when": "$(basename).ts"
    }
  }
}
